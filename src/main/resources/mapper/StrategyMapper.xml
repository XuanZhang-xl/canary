<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.xl.canary.mapper.CouponMapper">

    <select id="listAvailableStrategiesBySubject" resultType="com.xl.canary.entity.StrategyEntity">
        select * from t_canary_strategy WHERE subject = #{subject} and effective_date &lt;= #{time} and expire_date &gt; #{time}
    </select>

</mapper>